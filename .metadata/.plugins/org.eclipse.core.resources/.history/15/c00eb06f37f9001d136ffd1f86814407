package Array_List;

import java.util.ArrayList;

public class Array_List_Demo_2 {

	public static void main(String[] args) {
		
		ArrayList al = new ArrayList();
		al.add("A");
		al.add("B");
		al.add("C");
		al.add("D");
		
		System.out.println(al);
		
		ArrayList al2 = new ArrayList();
		al2.addAll(al);
		
		System.out.println(al2);
	}
}
